{
  "exp": {},
  "model": {
    "name": "bigan.BIGAN"
  },
  "dirs": {
    "data": "data",
    "data_normal": "data/Normal/",
    "data_anomalous": "data/Anomalous/"
  },
  "data_loader": {
    "name": "data_generator.DataGenerator",
    "buffer_size": 51200,
    "image_size": 28,
    "batch_size": 64,
    "num_epochs": 100,
    "num_iter_per_epoch": 800,
    "num_parallel_calls": 8,
    "num_iter_per_test": 3337,
    "mode": "anomaly",
    "dataset_name": "material",
    "binary_location": "offload",
    "test_batch": 40,
     "validation": true,
    "validation_percent": 20
  },
  "trainer": {
    "name": "bigan_trainer.BIGANTrainer",
    "noise_dim": 100,
    "image_dims": [28, 28, 1],
    "include_noise": false,
    "soft_labels": true,
    "flip_labels": true,
    "noise_probability": 0.05,
    "discriminator_l_rate": 2e-5,
    "generator_l_rate": 2e-4,
    "optimizer_adam_beta1": 0.5,
    "optimizer_adam_beta2": 0.999,
    "batch_momentum": 0.8,
    "leakyReLU_alpha": 0.25,
    "dis_inter_layer_dim": 1024,
    "loss_method": "fm",
    "dropout_rate": 0.5,
    "ema_decay": 0.999,
    "spectral_norm": true,
    "allow_zz": true,
    "enable_early_stop": true,
    "degree": 1,
    "weight": 0.1,
    "label": 0,
    "frequency_eval": 2,
    "patience": 10,
    "test_at_end": true,
    "percentiles": [80, 85, 90, 95, 96, 97, 98, 99, 99.1, 99.2, 99.3, 99.4, 99.5, 99.6, 99.7, 99.8, 99.9]
  },
  "log": {
    "name": "summarizer.Summarizer",
    "enable_summary": true,
    "show_steps": 10,
    "max_to_keep": 2,
    "num_example_imgs_to_generate": 25,
    "frequency_test": 5,
    "output_folder": "Results"
  }
}